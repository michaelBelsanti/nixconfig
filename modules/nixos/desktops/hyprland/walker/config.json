{
  "placeholder": "Search...",
  "notify_on_fail": true,
  "show_initial_entries": true,
  "orientation": "vertical",
  "terminal": "footclient",
  "fullscreen": true,
  "modules": [
    {
      "name": "applications"
    },
    {
      "prefix": "?",
      "name": "websearch"
    },
    {
      "prefix": "/",
      "name": "hyprland"
    },
    {
      "name": "external"
    }
  ],
  "external": [
    {
      "label": "Shut down",
      "exec": "systemctl poweroff"
    },
    {
      "label": "Reboot",
      "exec": "systemctl reboot",
      "searchable": "restart",
      "notifyable": true
    },
    {
      "label": "Suspend",
      "exec": "systemctl suspend",
      "searchable": "sleep",
      "notifyable": true
    },
    {
      "label": "Log out",
      "exec": "remindme in 1s test",
      "searchable": "log off",
      "notifyable": true
    }
  ],
  "icons": {
    "size": 28,
    "image_height": 200
  },
  "align": {
    "horizontal": "center",
    "vertical": "start",
    "width": 400,
    "margins": {
      "top": 20
    }
  },
  "list": {
    "margin_top": 10,
    "height": 300,
    "always_show": true
  },
  "search": {},
  "clipboard": {
    "image_height": 300,
    "max_entries": 10
  }
}